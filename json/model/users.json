{
    "name": "用户表",
    "table": "users",
    "joins": [
        {
            "table": "master_university",
            "foreign": "id",
            "key": "master_university_id",
            "join": "left",
            "columns": [
                {
                    "field": "name"
                }
            ],
            "wheres": [
                {
                    "field": "deleted_at",
                    "match": "is",
                    "value": "null"
                },
                {
                    "field": "flag_show",
                    "match": "=",
                    "value": "1"
                }
            ]
        }
    ],
    "withs": [],
    "withs_count": [],
    "withs_sum": [],
    "columns": [
        {
            "field": "id"
        },
        {
            "field": "name"
        },
        {
            "field": "email"
        },
        {
            "field": "master_university_id"
        },
        {
            "field": "master_job_id"
        }
    ],
    "wheres": [
        {
            "field": "created_at",
            "match": "between",
            "value": "2023-01-01~2023-10-01"
        }
    ]
}